    "version": "0.2.0",
    "configurations": [
        

        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: Configure project",
            "cmakeDebugType": "configure",
            "clean": false,
            "configureAll": false
        },
        
        {
            "name": "C/C++ Debug (gdb Launch)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/debug/wiki-case",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}